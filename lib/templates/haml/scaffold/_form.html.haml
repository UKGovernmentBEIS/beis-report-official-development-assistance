-# frozen_string_literal: true
= form_for model: @<%= singular_table_name %> do |f|
  = f.govuk_error_summary
 
  .form-inputs
  <%- attributes.each do |attribute| -%>
    = f.<%= attribute.reference? ? :association : :input %> :<%= attribute.name %>
  <%- end -%>

  .form-actions
    = f.button :submit
